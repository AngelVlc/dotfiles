#############################
# alias
#############################
alias gb='git branch -l'
alias gc='git checkout'
alias gcm='git checkout master'
alias gcs='git log origin/master..HEAD'
alias gd='git diff --cached'
alias gpb='git push --set-upstream origin'
alias gs='git status'
alias gp='git pull'
alias gl="git log --graph --pretty=format:'%C(magenta)%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
