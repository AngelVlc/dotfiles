#############################
# alias
#############################
alias ll='ls -l --color=auto'
alias lla='ls -la --color=auto'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'
alias tail='tail -f -n 50'

# find file recursive
fr(){
  find . -name $1 -print
}

# parse json
json(){
  echo $1 | python -m json.tool
}
