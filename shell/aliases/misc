#############################
# alias
#############################
alias ll='ls -l'
alias lla='ls -la'
alias egrep='egrep'
alias fgrep='fgrep'
alias grep='grep'
alias tail='tail -f -n 50'

# find file recursive
fr(){
  find . -name $1 -print
}

# parse json
json(){
  echo $1 | python -m json.tool
}
